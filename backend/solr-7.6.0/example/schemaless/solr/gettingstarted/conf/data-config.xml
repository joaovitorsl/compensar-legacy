<?xml version="1.0"?>
<dataConfig>
<dataSource name="MyMongo" type="MongoDataSource" database="aepc" />
<document name="import">
 <!-- if query="" then it imports everything -->
 
  <entity  processor="MongoEntityProcessor"
              query=""
             collection="usuario"   
             datasource="MyMongo"
             transformer="MongoMapperTransformer" name="usuario_entity">

               <!--  If mongoField name and the field declared in schema.xml are same than no need to declare below.
                     If not same than you have to refer the mongoField to field in schema.xml
                    ( Ex: mongoField="EmpNumber" to name="EmployeeNumber"). -->                                              

           <field column="_id"  name="id"/>               
                                                                                                                                                                                                       
	</entity>
  <entity  processor="MongoEntityProcessor"
              query=""
             collection="questao-subj"   
             datasource="MyMongo"
             transformer="MongoMapperTransformer" name="questao-subj_entity">

               <!--  If mongoField name and the field declared in schema.xml are same than no need to declare below.
                     If not same than you have to refer the mongoField to field in schema.xml
                    ( Ex: mongoField="EmpNumber" to name="EmployeeNumber"). -->                                              

           <field column="_id"  name="id"/>               
                                                                                                                                                                                                       
	</entity>
  <entity  processor="MongoEntityProcessor"
              query=""
             collection="questao-obj"   
             datasource="MyMongo"
             transformer="MongoMapperTransformer" name="questao-obj_entity">

               <!--  If mongoField name and the field declared in schema.xml are same than no need to declare below.
                     If not same than you have to refer the mongoField to field in schema.xml
                    ( Ex: mongoField="EmpNumber" to name="EmployeeNumber"). -->                                              

           <field column="_id"  name="id"/>               
                                                                                                                                                                                                       
	</entity>

</document>
 


</dataConfig>
